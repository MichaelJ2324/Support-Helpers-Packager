{
    "name": "sugarcrm/Support-Helpers-Packager",
    "description": "Helpers for packaging Sugar",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://github.com/sugarcrm/Support-Helpers-Packager",
    "support": {
        "wiki": "http://support.sugarcrm.com",
        "forum": "https://community.sugarcrm.com/sugarcrm",
        "irc": "irc://irc.freenode.org/sugarcrm"
    },
    "keywords": ["sugarcrm","sugar"],
    "authors": [
        {
            "name": "SugarCRM",
            "homepage": "http://www.sugarcrm.com"
        }
    ],
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "frizinak/ZipStreamer",
                "version":"dev-master",
                "source": {
                    "url": "https://github.com/frizinak/ZipStreamer",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.2",
        "frizinak/ZipStreamer": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Sugarcrm\\Support\\Helpers\\Packager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sugarcrm\\Support\\Helpers\\Packager\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
